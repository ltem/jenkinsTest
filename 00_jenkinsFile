pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'test_jenkinsFile', url: 'https://github.com/ltem/jenkinsTest.git'
                
            }
        }
        stage('Build') {
            steps {
                python3 writeTestFile.py
                
            }
        }
        stage('Test') {
            steps {
                echo 'Test successful'
                
            }
        }
    }
}

