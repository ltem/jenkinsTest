pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'test_jenkinsFile', url: 'https://github.com/ltem/jenkinsTest.git'
                sh 'pwd'
                sh 'ls -l'
            }
        }
        stage('Build') {
            steps {
                sh 'python3 writeTestFile.py'
                
            }
        }
        stage('Test') {
            steps {
                sh 'cat ./*'
                echo 'Test successful'
                
            }
        }
    }
}

